<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!-- Label for item that reboots the device in device options dialog -->
    <!-- 电源菜单中的"重新启动"按钮 -->
    <string name="global_action_reboot">重新启动</string>
    <!-- Label for item that screenshots in device options dialog -->
    <!-- 电源菜单中的"屏幕截图"按钮 -->
    <string name="global_action_screenshot">屏幕截图</string>

    <!-- Button to reboot the device, within the reboot options dialog -->
    <!-- "重新启动"对话框中的标准重启按钮 -->
    <string name="reboot_normal">@string/global_action_reboot</string>
    <!-- Button to reboot the device into recovery, within the reboot options dialog -->
    <!-- "重新启动"对话框中的重启至恢复模式按钮 -->
    <string name="reboot_recovery">恢复模式</string>
    <!-- Button to reboot the device into fastboot, within the reboot options dialog -->
    <!-- "重新启动"对话框中的重启至Fastboot模式按钮 -->
    <string name="reboot_fastboot">引导模式</string>
    <!-- Reboot confirmation dialog text -->
    <!-- 确认重启的对话框文字 -->
    <string name="reboot_confirm" product="tablet">你的Nexus将重新启动</string>
    <string name="reboot_confirm" product="default">你的Nexus将重新启动</string>
    <!-- Reboot progress dialog text -->
    <!-- 重启过程的对话框文字 -->
    <string name="reboot_progress">正在重新启动...\u2026</string>

    <!-- Shutdown confirmation dialog text -->
    <!-- 确认关机的对话框文字 -->
    <string name="shutdown_confirm" product="tablet">你的Nexus将关机</string>
    <string name="shutdown_confirm" product="default">你的Nexus将关机</string>

    <!-- Immersive mode confirmation: Pick confirmation window from bottom when status bar is visible -->
    <!-- 沉浸模式确认: 当通知栏可见时 确认窗口会从屏幕下端弹出 -->
    <string name="immersive_mode_confirmation_bottom">从底部滑出以退出全屏</string>

    <!-- Longpress back button to kill foreground app -->
    <!-- 长按返回按钮以杀掉前台应用 -->
    <string name="app_killed_message">应用的人头给你带来了</string>

    <!-- Setting confirmation dialog -->
    <!-- 设置确认对话框 -->
    <string name="setting_confirmation_ask_me_later">稍候询问</string>
    <string name="setting_confirmation_yes">是</string>
    <string name="setting_confirmation_no">否</string>

</resources>
