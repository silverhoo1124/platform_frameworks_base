<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!-- Label for item that reboots the device in device options dialog -->
    <!-- 电源菜单中的"重新启动"按钮 -->
    <string name="global_action_reboot">Reboot</string>
    <!-- Label for item that screenshots in device options dialog -->
    <!-- 电源菜单中的"屏幕截图"按钮 -->
    <string name="global_action_screenshot">Screenshot</string>

    <!-- Button to reboot the device, within the reboot options dialog -->
    <!-- "重新启动"对话框中的标准重启按钮 -->
    <string name="reboot_normal">@string/global_action_reboot</string>
    <!-- Button to reboot the device into recovery, within the reboot options dialog -->
    <!-- "重新启动"对话框中的重启至恢复模式按钮 -->
    <string name="reboot_recovery">Recovery</string>
    <!-- Button to reboot the device into fastboot, within the reboot options dialog -->
    <!-- "重新启动"对话框中的重启至Fastboot模式按钮 -->
    <string name="reboot_fastboot">Fastboot</string>
    <!-- Reboot confirmation dialog text -->
    <!-- 确认重启的对话框文字 -->
    <string name="reboot_confirm" product="tablet">Your Nexus will reboot.</string>
    <string name="reboot_confirm" product="default">Your Nexus will reboot.</string>
    <!-- Reboot progress dialog text -->
    <!-- 重启过程的对话框文字 -->
    <string name="reboot_progress">Rebooting\u2026</string>

    <!-- Shutdown confirmation dialog text -->
    <!-- 确认关机的对话框文字 -->
    <string name="shutdown_confirm" product="tablet">Your Nexus will shut down.</string>
    <string name="shutdown_confirm" product="default">Your Nexus will shut down.</string>

    <!-- Immersive mode confirmation: Pick confirmation window from bottom when status bar is visible -->
    <!-- 沉浸模式确认: 当通知栏可见时 确认窗口会从屏幕下端弹出 -->
    <string name="immersive_mode_confirmation_bottom">Swipe up from the bottom to exit full screen.</string>

    <!-- Longpress back button to kill foreground app -->
    <!-- 长按返回按钮以杀掉前台应用 -->
    <string name="app_killed_message">App killed</string>

    <!-- Setting confirmation dialog -->
    <!-- 设置确认对话框 -->
    <string name="setting_confirmation_ask_me_later">Ask later</string>
    <string name="setting_confirmation_yes">Yes</string>
    <string name="setting_confirmation_no">No</string>

</resources>
