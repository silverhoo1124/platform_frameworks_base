<resources>

  <!-- Power menu -->
  <!-- 电源菜单 -->
  <java-symbol type="array" name="shutdown_reboot_options" />
  <java-symbol type="array" name="shutdown_reboot_actions" />
  <java-symbol type="array" name="shutdown_reboot_icons" />
  <java-symbol type="drawable" name="ic_lock_reboot" />
  <java-symbol type="drawable" name="ic_lock_reboot_recovery" />
  <java-symbol type="drawable" name="ic_lock_reboot_bootloader" />
  <java-symbol type="drawable" name="ic_lock_screenshot" />
  <java-symbol type="string" name="global_actions" />
  <java-symbol type="string" name="global_action_reboot" />
  <java-symbol type="string" name="global_action_screenshot" />
  <java-symbol type="string" name="reboot_confirm" />
  <java-symbol type="string" name="reboot_progress" />

  <!-- config.xml entries for camera flash check -->
  <!-- config.xml中闪光灯检测的条目 -->
  <java-symbol type="bool" name="config_device_has_camera_flash" />

  <!-- config.xml entries for foreground app kill -->
  <!-- config.xml中杀掉前台应用的条目 -->
  <java-symbol type="integer" name="config_backKillTimeout" />
  <!-- Developer settings - longpress back button to kill foreground app -->
  <!-- 开发者选项 - 长按返回键杀掉前台应用 -->
  <java-symbol type="string" name="app_killed_message" />

  <!-- Navigation ring targets customization -->
  <!-- 个性化导航圈目标 -->
  <java-symbol type="drawable" name="ic_navigation_ring_blank_normal" />
  <java-symbol type="drawable" name="ic_navigation_ring_blank_activated" />
  <java-symbol type="drawable" name="ic_navigation_ring_empty" />
  <java-symbol type="drawable" name="ic_navigation_ring_ime_switcher" />
  <java-symbol type="drawable" name="ic_navigation_ring_killtask" />
  <java-symbol type="drawable" name="ic_navigation_ring_power" />
  <java-symbol type="drawable" name="ic_navigation_ring_screenshot" />
  <java-symbol type="drawable" name="ic_navigation_ring_silent" />
  <java-symbol type="drawable" name="ic_navigation_ring_sound_on" />
  <java-symbol type="drawable" name="ic_navigation_ring_vibrate" />
  <java-symbol type="drawable" name="ic_navigation_ring_power_menu" />

  <!-- LED customization -->
  <!-- 个性化LED -->
  <java-symbol type="array" name="notification_light_package_mapping" />

  <!-- Recent panel -->
  <!-- 最近应用面板 -->
  <java-symbol type="integer" name="config_recentDefaultDur" />
  <java-symbol type="integer" name="config_recentExitDur" />
  <java-symbol type="style" name="Animation.RecentScreen" />
  <java-symbol type="style" name="Animation.RecentScreen.Left" />
  <java-symbol type="anim" name="recent_screen_enter" />
  <java-symbol type="anim" name="recent_screen_enter_left" />
  <java-symbol type="anim" name="recent_screen_fade_out" />

  <!-- Setting confirmation dialog -->
  <!-- 设置确认对话框 -->
  <java-symbol type="id" name="setting_confirmation_dialog_visual_hint" />
  <java-symbol type="layout" name="setting_confirmation_dialog" />
  <java-symbol type="string" name="setting_confirmation_ask_me_later" />
  <java-symbol type="string" name="setting_confirmation_yes" />
  <java-symbol type="string" name="setting_confirmation_no" />

</resources>
